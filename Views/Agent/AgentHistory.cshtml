@model IEnumerable<UDS.Models.AgencyList>

<div>
    <div>
        <ul style="list-style:none; width:100%; min-width:600px;">
            <li style="width:30%; float:left; text-align:center">授权人</li>
            <li style="width:35%; float:left; text-align:center">开始时间</li>
            <li style="width:35%; float:left; text-align:center">结束时间</li>
        </ul>
        <br />
        <div>
            @foreach (var item in Model)
            {
                <ul style="list-style:none; width:100%; min-width:600px;">
                    <li style="width:30%; float:left; text-align:center">
                        @Html.DisplayFor(modelItem => item.GrantorName)
                    </li>
                    <li style="width:35%; float:left; text-align:center">
                        @Html.DisplayFor(modelItem => item.BeginTime)
                    </li>
                    <li style="width:35%; float:left; text-align:center">
                        @Html.DisplayFor(modelItem => item.EndTime)
                    </li>
                </ul>
                <br />
            }
        </div>
    </div>
</div>

